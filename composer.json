{
    "name": "clarity-tech/laravel-ezyslips",
    "description": "Laravel ezyslips package",
    "license": "MIT",
    "authors": [
        {
            "name": "Manash Sonowal",
            "email": "manash149@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.3.0",
        "illuminate/support": "^7.0|^8.0|^9.0|10.0",
        "psr/http-message": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "ClarityTech\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ClarityTech\\Ezyslips\\EzyslipsServiceProvider"
            ],
            "aliases": {
                "Ezyslips": "ClarityTech\\Ezyslips\\Facades\\Ezyslips"
            }
        }
    },
    "suggest": {
        "guzzlehttp/guzzle": "Must Required to use the HTTP Client."
    },
    "config": {
        "preferred-install": "dist"
    }
}
